<h1> User Registration </h1>

<%= erb :_errors %>

<form method="post" action="/users">

  <div>
    <input type="email" name="user[email]" placeholder="Email">
  </div>

  <div>
    <input type="text" name="user[password]" placeholder="Password">
  </div>

  <div>
    <input type="submit" value="Create User">
  </div>

</form>
